{
  "listener": {
    "name": "ble",
    "hci-device-id": 0,
    "ble-name": "XPI-SETUP",
    "ble-uuid": ""
  },

  "services": [
    {
      "name": "wifi",
      "settings": {
        "interface": "/var/run/wpa_supplicant/wlan0"
      }
    },

    {
      "name": "config",
      "settings":{
        "db-file": "bleconfd.ini"
      }
    },

    {
      "name": "cmd",
      "settings": {
        "commands": [
          {
            "name": "test-one",
            "exec": "/bin/ls"
          },

          {
            "name": "test-two",
            "exec": "/bin/pwd"
          }
        ]
      }
    }
  ]
}
